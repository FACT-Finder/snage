{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "rangeStrategy": "pin",
  "groupName": "all",
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": [
      "on the 14 day of the month"
    ],
    "groupName": "all-lock"
  },
  "packageRules": [
    {
      "matchPackageNames": [
        "copy-webpack-plugin"
      ],
      "allowedVersions": "<=9.1.0"
    },
    {
      "matchPackageNames": [
        "env-editor"
      ],
      "allowedVersions": "<=0.5.0"
    }
  ],
  "schedule": [
    "on the 1 day of the month"
  ]
}
