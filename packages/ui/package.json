  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.13.0",
    "@mui/styles": "5.12.3",
    "@types/axios": "0.14.0",
    "@types/node": "20.1.5",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "axios": "1.4.0",
    "github-markdown-css": "5.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.7",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "15.5.0",
    "react-virtualized": "9.22.5",
    "remark-gfm": "3.0.1",
    "typescript": "5.0.4",
    "use-debounce": "9.0.4"
  },
  "scripts": {
    "lint-check": "eslint -c \"../../eslint.yaml\" \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "yarn run lint-check --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "rimraf build/**/*.map",
    "eject": "react-scripts eject",
    "format": "prettier --config \"../../prettier.config.js\" \"src/**/*.{ts,tsx}\" --write",
    "format-check": "prettier --config \"../../prettier.config.js\" \"src/**/*.{ts,tsx}\" --list-different"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-syntax-highlighter": "15.5.6",
    "@types/react-virtualized": "9.21.21",
    "prettier": "2.8.8",
    "rimraf": "5.0.0"
  }
