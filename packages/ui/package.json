  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.3",
    "@mui/styles": "5.11.2",
    "@types/axios": "0.14.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "axios": "1.2.2",
    "github-markdown-css": "5.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.4",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "15.5.0",
