  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.12.2",
    "@material-ui/icons": "4.11.2",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.2.0",
    "@types/axios": "0.14.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.14",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.9",
    "axios": "0.21.1",
    "github-markdown-css": "5.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "8.0.0",
    "react-scripts": "5.0.0",
    "react-syntax-highlighter": "15.4.5",
    "react-virtualized": "9.22.3",
    "typescript": "4.3.5",
    "use-debounce": "7.0.0"
  },
  "scripts": {
    "lint-check": "eslint -c \"../../eslint.yaml\" \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "yarn run lint-check --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "rimraf build/**/*.map",
    "eject": "react-scripts eject",
    "format": "prettier --config \"../../prettier.config.js\" \"src/**/*.{ts,tsx}\" --write",
    "format-check": "prettier --config \"../../prettier.config.js\" \"src/**/*.{ts,tsx}\" --list-different"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react-syntax-highlighter": "13.5.2",
    "@types/react-virtualized": "9.21.12",
    "prettier": "2.3.2",
    "rimraf": "3.0.2"
  }
