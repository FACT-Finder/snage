  "private": true,
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "6.4.11",
    "@mui/material": "6.4.11",
    "@mui/styles": "6.4.8",
    "@types/axios": "0.14.4",
    "@types/node": "22.15.21",
    "@types/react": "19.1.5",
    "@types/react-dom": "19.1.5",
    "axios": "1.9.0",
    "github-markdown-css": "5.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-markdown": "9.1.0",
    "react-scripts": "5.0.1",
    "react-syntax-highlighter": "15.6.1",
    "react-virtualized": "9.22.6",
    "remark-gfm": "4.0.1",
    "typescript": "5.8.3",
    "use-debounce": "10.0.4"
  },
  "scripts": {
    "lint-check": "eslint -c \"../../eslint.yaml\" \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "yarn run lint-check --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "rimraf build/**/*.map",
    "eject": "react-scripts eject",
    "format": "prettier --config \"../../prettier.config.js\" \"src/**/*.{ts,tsx}\" --write",
    "format-check": "prettier --config \"../../prettier.config.js\" \"src/**/*.{ts,tsx}\" --list-different"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@types/react-syntax-highlighter": "15.5.13",
    "@types/react-virtualized": "9.22.2",
    "prettier": "3.5.3",
    "rimraf": "6.0.1"
  }
}
