{
  "name": "snage",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "lint-check": "npm-run-all eslint-check snagelint",
    "lint": "npm-run-all eslint snagelint",
    "eslint-check": "eslint -c \"../../eslint.yaml\" \"src/**/*.ts\"",
    "eslint": "yarn run eslint-check --fix",
    "snagelint": "yarn run dev --config ../../.snage.yaml lint",
    "dev-serve": "cross-env SNAGE_CONFIG=../../.snage.yaml webpack --config webpack.dev.js --watch",
    "dev": "ts-node src/index.ts",
    "test": "jest --coverage 'src/.*\\.test\\.ts'",
    "build": "webpack --config webpack.prod.js",
    "format": "prettier --config \"../../prettier.config.js\" \"*.js\" \"src/**/*.ts\" --write",
    "format-check": "prettier --config \"../../prettier.config.js\" \"*.js\" \"src/**/*.ts\" --list-different"
  },
  "files": [
    "build/npm/**"
  ],
  "bin": "build/npm/snage.js",
  "dependencies": {
    "@js-joda/core": "^2.0.0",
    "ajv": "^6.12.2",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "fp-ts": "^2.5.3",
    "io-ts": "^2.2.1",
    "node-fetch": "^2.6.0",
    "parsimmon": "^1.13.0",
    "prom-client": "^12.0.0",
    "semver": "^7.1.3",
    "string-similarity": "^4.0.1",
    "winston": "^3.2.1",
    "yaml": "^1.9.2",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.4",
    "@types/get-port": "^4.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.19",
    "@types/nock": "^11.1.0",
    "@types/node": "^12.7.5",
    "@types/node-fetch": "^2.5.1",
    "@types/parsimmon": "^1.10.1",
    "@types/query-string": "^6.3.0",
    "@types/semver": "^7.1.0",
    "@types/string-similarity": "^3.0.0",
    "@types/supertest": "^2.0.8",
    "@types/winston": "^2.4.4",
    "@types/yaml": "^1.9.7",
    "copy-webpack-plugin": "^5.1.1",
    "env-editor": "^0.4.1",
    "get-port": "^5.0.0",
    "jest": "^24.9.0",
    "jest-junit": "^9.0.0",
    "nock": "^11.6.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "portable-fetch": "^3.0.0",
    "prettier": "^1.18.2",
    "snage-ui": "*",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.4.1",
    "tslib": "^1.10.0",
    "tslint": "^5.20.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.8.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.10.3",
    "webpack-shell-plugin": "^0.5.0"
  }
}
