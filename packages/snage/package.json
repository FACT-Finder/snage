{
  "name": "snage",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "lint-check": "npm-run-all eslint-check snagelint",
    "lint": "npm-run-all eslint snagelint",
    "eslint-check": "eslint -c \"../../eslint.yaml\" \"src/**/*.ts\"",
    "eslint": "yarn run eslint-check --fix",
    "snagelint": "yarn run dev --config ../../.snage.yaml lint",
    "dev-serve": "cross-env SNAGE_CONFIG=../../.snage.yaml webpack --config webpack.dev.js --watch",
    "dev": "ts-node src/index.ts",
    "test": "jest --coverage 'src/.*\\.test\\.ts'",
    "build": "webpack --config webpack.prod.js",
    "format": "prettier --config \"../../prettier.config.js\" \"*.js\" \"src/**/*.ts\" --write",
    "format-check": "prettier --config \"../../prettier.config.js\" \"*.js\" \"src/**/*.ts\" --list-different"
  },
  "files": [
    "build/npm/**"
  ],
  "bin": "build/npm/snage.js",
  "dependencies": {
    "@js-joda/core": "5.2.0",
    "ajv": "8.11.0",
    "cross-env": "7.0.3",
    "express": "4.17.3",
    "fp-ts": "2.11.9",
    "io-ts": "2.2.16",
    "parsimmon": "1.18.1",
    "prom-client": "14.0.1",
    "semver": "7.3.5",
    "string-similarity": "4.0.4",
    "winston": "3.7.2",
    "yaml": "1.10.2",
    "yargs": "17.4.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/express": "4.17.13",
    "@types/get-port": "4.2.0",
    "@types/inquirer": "8.2.1",
    "@types/jest": "27.4.1",
    "@types/nock": "11.1.0",
    "@types/node": "17.0.23",
    "@types/parsimmon": "1.10.6",
    "@types/query-string": "6.3.0",
    "@types/semver": "7.3.9",
    "@types/string-similarity": "4.0.0",
    "@types/supertest": "2.0.12",
    "@types/winston": "2.4.4",
    "@types/yaml": "1.9.7",
    "copy-webpack-plugin": "9.1.0",
    "env-editor": "0.5.0",
    "get-port": "6.1.2",
    "jest": "27.5.1",
    "jest-junit": "13.0.0",
    "lerna": "4.0.0",
    "nock": "13.2.4",
    "nodemon": "2.0.15",
    "npm-run-all": "4.1.5",
    "portable-fetch": "3.0.0",
    "prettier": "2.6.2",
    "snage-ui": "*",
    "supertest": "6.2.2",
    "ts-jest": "27.1.4",
    "ts-loader": "9.2.8",
    "ts-node": "10.7.0",
    "tslib": "2.3.1",
    "typescript": "4.6.3",
    "webpack": "5.71.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4",
    "webpack-shell-plugin-next": "2.2.2"
  }
}
