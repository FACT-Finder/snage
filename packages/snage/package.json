{
  "name": "snage",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "lint-check": "npm-run-all eslint-check snagelint",
    "lint": "npm-run-all eslint snagelint",
    "eslint-check": "eslint -c \"../../eslint.yaml\" \"src/**/*.ts\"",
    "eslint": "yarn run eslint-check --fix",
    "snagelint": "yarn run dev --config ../../.snage.yaml lint",
    "dev-serve": "cross-env SNAGE_CONFIG=../../.snage.yaml webpack --config webpack.dev.js --watch",
    "dev": "ts-node src/index.ts",
    "test": "jest --coverage 'src/.*\\.test\\.ts'",
    "build": "webpack --config webpack.prod.js",
    "format": "prettier --config \"../../prettier.config.js\" \"*.js\" \"src/**/*.ts\" --write",
    "format-check": "prettier --config \"../../prettier.config.js\" \"*.js\" \"src/**/*.ts\" --list-different"
  },
  "files": [
    "build/npm/**"
  ],
  "bin": "build/npm/snage.js",
  "dependencies": {
    "@js-joda/core": "5.5.3",
    "ajv": "8.12.0",
    "cross-env": "7.0.3",
    "express": "4.18.2",
    "fp-ts": "2.16.0",
    "io-ts": "2.2.20",
    "parsimmon": "1.18.1",
    "prom-client": "14.2.0",
    "semver": "7.5.1",
    "string-similarity": "4.0.4",
    "winston": "3.9.0",
    "yaml": "2.3.1",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/express": "4.17.17",
    "@types/get-port": "4.2.0",
    "@types/inquirer": "8.2.6",
    "@types/jest": "29.5.2",
    "@types/nock": "11.1.0",
    "@types/node": "20.2.5",
    "@types/parsimmon": "1.10.6",
    "@types/query-string": "6.3.0",
    "@types/semver": "7.5.0",
    "@types/string-similarity": "4.0.0",
    "@types/supertest": "2.0.12",
    "@types/winston": "2.4.4",
    "@types/yaml": "1.9.7",
    "copy-webpack-plugin": "9.1.0",
    "env-editor": "0.5.0",
    "get-port": "6.1.2",
    "jest": "29.5.0",
    "jest-junit": "16.0.0",
    "lerna": "6.6.2",
    "nock": "13.3.1",
    "nodemon": "2.0.22",
    "npm-run-all": "4.1.5",
    "portable-fetch": "3.0.0",
    "prettier": "2.8.8",
    "snage-ui": "*",
    "supertest": "6.3.3",
    "ts-jest": "29.1.0",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tslib": "2.5.3",
    "typescript": "5.1.3",
    "webpack": "5.85.0",
    "webpack-cli": "5.1.3",
    "webpack-dev-server": "4.15.0",
    "webpack-shell-plugin-next": "2.3.1"
  }
}
