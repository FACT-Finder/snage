{
  "name": "snage",
  "license": "MIT",
  "version": "0.0.1",
  "scripts": {
    "lint-check": "npm-run-all eslint-check snagelint",
    "lint": "npm-run-all eslint snagelint",
    "eslint-check": "eslint -c \"../../eslint.yaml\" \"src/**/*.ts\"",
    "eslint": "yarn run eslint-check --fix",
    "snagelint": "yarn run dev --config ../../.snage.yaml lint",
    "dev-serve": "cross-env SNAGE_CONFIG=../../.snage.yaml webpack --config webpack.dev.js --watch",
    "dev": "ts-node src/index.ts",
    "test": "jest --coverage 'src/.*\\.test\\.ts'",
    "build": "webpack --config webpack.prod.js",
    "format": "prettier --config \"../../prettier.config.js\" \"*.js\" \"src/**/*.ts\" --write",
    "format-check": "prettier --config \"../../prettier.config.js\" \"*.js\" \"src/**/*.ts\" --list-different"
  },
  "files": [
    "build/npm/**"
  ],
  "bin": "build/npm/snage.js",
  "dependencies": {
    "@js-joda/core": "3.2.0",
    "ajv": "8.6.2",
    "cross-env": "7.0.3",
    "express": "4.17.1",
    "fp-ts": "2.10.5",
    "io-ts": "2.2.16",
    "node-fetch": "2.6.1",
    "parsimmon": "1.18.0",
    "prom-client": "13.1.0",
    "semver": "7.3.5",
    "string-similarity": "4.0.4",
    "winston": "3.3.3",
    "yaml": "1.10.2",
    "yargs": "17.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.1",
    "@types/express": "4.17.13",
    "@types/get-port": "4.2.0",
    "@types/inquirer": "7.3.3",
    "@types/jest": "26.0.24",
    "@types/nock": "11.1.0",
    "@types/node": "16.4.1",
    "@types/node-fetch": "2.5.12",
    "@types/parsimmon": "1.10.6",
    "@types/query-string": "6.3.0",
    "@types/semver": "7.3.7",
    "@types/string-similarity": "4.0.0",
    "@types/supertest": "2.0.11",
    "@types/winston": "2.4.4",
    "@types/yaml": "1.9.7",
    "copy-webpack-plugin": "9.0.1",
    "env-editor": "0.5.0",
    "get-port": "5.1.1",
    "jest": "27.0.6",
    "jest-junit": "12.2.0",
    "lerna": "4.0.0",
    "nock": "13.1.1",
    "nodemon": "2.0.12",
    "npm-run-all": "4.1.5",
    "portable-fetch": "3.0.0",
    "prettier": "2.3.2",
    "snage-ui": "*",
    "supertest": "6.1.4",
    "ts-jest": "27.0.4",
    "ts-loader": "9.2.3",
    "ts-node": "10.1.0",
    "tslib": "2.3.0",
    "typescript": "4.3.5",
    "webpack": "5.46.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "3.11.2",
    "webpack-shell-plugin-next": "2.2.2"
  }
}
